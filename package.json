{
  "name": "Pocipe-backend",
  "version": "1.0.0",
  "description": "GraphQL + Prisma + Apollo Server",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "dev": "nodemon --exec babel-node src/server",
