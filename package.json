    "@zvs001/s3-utils": "^0.1.0",
    "apollo-server": "^3.1.1",
    "apollo-server-core": "^3.1.2",
    "apollo-server-express": "^3.1.1",
    "aws-sdk": "^2.972.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
